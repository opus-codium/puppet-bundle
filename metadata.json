{
  "name": "opuscodium-bundle",
  "version": "0.0.0",
  "author": "Opus Codium",
  "summary": "Installs bundler and allow bundle execution",
  "license": "Apache-2.0",
  "source": "https://github.com/opus-codium/puppet-bundle",
  "project_page": "https://github.com/opus-codium/puppet-bundle",
  "issues_url": "https://github.com/opus-codium/puppet-bundle/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 2.6.0 < 9.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "11",
        "12",
        "13"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.0.0 < 8.0.0"
    }
  ]
}
